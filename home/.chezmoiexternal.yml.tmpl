{{ if ne .chezmoi.os "windows" -}}
".oh-my-zsh":
  type: archive
  url: "https://github.com/ohmyzsh/ohmyzsh/archive/master.tar.gz"
  stripComponents: 1
  refreshPeriod: "168h"
  exclude:
    - "*/.*"
    - "*/templates"
    - "*/themes"
".oh-my-zsh/custom/themes/spaceship-prompt":
  type: "archive"
  url: "https://github.com/spaceship-prompt/spaceship-prompt/archive/master.tar.gz"
  exact: true
  stripComponents: 1
  refreshPeriod: "168h"
  exclude:
    - "*/.*"
    - "*/docs"
    - "scripts"
    - "tests"
".oh-my-zsh/custom/plugins/spaceship-vi-mode":
  type: archive
  url: "https://github.com/spaceship-prompt/spaceship-vi-mode/archive/master.tar.gz"
  exact: true
  stripComponents: 1
  refreshPeriod: "168h"
".oh-my-zsh/custom/plugins/zsh-syntax-highlighting":
  type: archive
  url: "https://github.com/zsh-users/zsh-syntax-highlighting/archive/master.tar.gz"
  exact: true
  stripComponents: 1
  refreshPeriod: "168h"
".oh-my-zsh/custom/plugins/zsh-autosuggestions":
  type: archive
  url: "https://github.com/zsh-users/zsh-autosuggestions/archive/master.tar.gz"
  stripComponents: 1
  exact: true
  refreshPeriod: "168h"
".oh-my-zsh/custom/plugins/zsh-completions":
  type: archive
  url: "https://github.com/zsh-users/zsh-completions/archive/master.tar.gz"
  stripComponents: 1
  exact: true
  refreshPeriod: "168h"
".oh-my-zsh/custom/plugins/fzf-tab":
  type: archive
  url: "https://github.com/Aloxaf/fzf-tab/archive/master.tar.gz"
  stripComponents: 1
  exact: true
  refreshPeriod: "168h"
".config/tmux/plugins/tpm":
  type: archive
  url: "https://github.com/tmux-plugins/tpm/archive/master.tar.gz"
  stripComponents: 1
  exact: true
  refreshPeriod: "168h"
{{ if lookPath "warp" }}
{{ if .chezmoi.os "linux" }}
".local/share/warp-terminal/themes/catppuccin_mocha.yml":
{{ else if .chezmoi.os "darwin" }}
".warp/themes/catppuccin_mocha.yml":
{{ end }}
  type: file
  url: "https://github.com/catppuccin/warp/raw/main/themes/catppuccin_mocha.yml"
  refreshPeriod: "168h"
{{ end }}
{{ end }}
